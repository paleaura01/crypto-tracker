{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "preserveValueImports": false
  },
  "include": [
    "src/**/*",
    "env.d.ts",
    "global.d.ts",
    "src/app.d.ts"
  ]
}
